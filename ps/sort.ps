% function to duplicate entire stack
/dup_stack {
    1 dict begin

    /n count 1 sub 1 sub def

    count {
        n index
    } repeat

    end
} def

% function that calculates the minimum of the top two numbers on the stack
% and pushes that min onto the stack
/min {
    1 dict begin

    /y exch def
    /x exch def

    x y lt
        {x}
        {y}
    ifelse
    end
} def

% param: number of times to call min
% calls min @param times
/find_smallest {

    % last item on the stack should be number of times to repeat
    { min } repeat

    end
} def

